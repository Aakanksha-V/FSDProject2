<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AdminController</servlet-name>
  	<display-name>AdminController</display-name>
  	<description></description>
  	<servlet-class>com.controller.AdminController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentController</servlet-name>
  	<display-name>StudentController</display-name>
  	<description></description>
  	<servlet-class>com.controller.StudentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TeacherController</servlet-name>
  	<display-name>TeacherController</display-name>
  	<description></description>
  	<servlet-class>com.controller.TeacherController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClassesController</servlet-name>
  	<display-name>ClassesController</display-name>
  	<description></description>
  	<servlet-class>com.controller.ClassesController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AdminController</servlet-name>
  	<url-pattern>/AdminController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentController</servlet-name>
  	<url-pattern>/StudentController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TeacherController</servlet-name>
  	<url-pattern>/TeacherController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClassesController</servlet-name>
  	<url-pattern>/ClassesController</url-pattern>
  </servlet-mapping>
</web-app>
